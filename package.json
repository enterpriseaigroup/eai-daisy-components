{
  "name": "eai-daisy-components",
  "version": "2.2.0",
  "description": "Component extraction pipeline for migrating DAISY v1 components to Configurator v2 architecture with business logic preservation",
  "type": "module",
  "scripts": {
    "build": "tsc --noEmit && vite build",
    "dev": "vite dev",
    "preview": "vite preview",
    "typecheck": "tsc --project tsconfig.ci.json --noEmit",
    "typecheck:strict": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "migrate:v2": "tsx packages/v2-components/src/cli/migrate-v2.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:types": "tsc --noEmit --project tsconfig.test.json",
    "test:integration:enhanced": "jest tests/integration/enhanced-production-readiness.test.ts",
    "test:integration:real": "jest tests/integration/integrated-production-readiness.test.ts --no-coverage",
    "test:smoke": "jest tests/integration/smoke-test.test.ts --no-coverage",
    "test:equivalency": "playwright test tests/equivalency",
    "test:equivalency:chromium": "playwright test tests/equivalency --project=chromium",
    "test:production": "npm run test:integration:real",
    "test:production:verbose": "jest tests/integration/integrated-production-readiness.test.ts --no-coverage --verbose",
    "test:performance": "jest tests/profilers --verbose",
    "test:bundles": "jest tests/analyzers/real-bundle-analyzer",
    "test:business-logic": "jest tests/analyzers/enhanced-business-logic-analyzer",
    "test:all-metrics": "jest tests/integration/integrated-production-readiness.test.ts --no-coverage --verbose",
    "ci": "npm run typecheck && npm run lint && npm run test && npm run build",
    "prepare": "husky install",
    "measure:baseline": "node scripts/measure-bundle-baseline.cjs",
    "migrate:component": "tsx src/cli/migrate-component.ts",
    "migrate:all": "tsx src/cli/migrate-all.ts",
    "generate:component": "npm run --prefix packages/v2-components generate:component",
    "generate:react": "npm run --prefix packages/v2-components generate:from-baseline"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^4.2.1",
    "compression-webpack-plugin": "^10.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.2",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0",
    "prettier": "^3.2.5",
    "terser-webpack-plugin": "^5.3.14",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.4",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^5.1.4"
  },
  "peerDependencies": {
    "@configurator/sdk": "^2.1.0"
  },
  "peerDependenciesMeta": {
    "@configurator/sdk": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "component-migration",
    "daisy-v1",
    "configurator-v2",
    "typescript",
    "react",
    "pipeline",
    "extraction"
  ],
  "author": "EAI Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eai/eai-daisy-components.git"
  },
  "bugs": {
    "url": "https://github.com/eai/eai-daisy-components/issues"
  },
  "homepage": "https://github.com/eai/eai-daisy-components#readme"
}
